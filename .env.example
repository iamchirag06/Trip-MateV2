# Database Configuration
# For local PostgreSQL:
# SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/tripmate
# SPRING_DATASOURCE_USERNAME=tripmate_user
# SPRING_DATASOURCE_PASSWORD=your_secure_database_password

# For Neon Database (recommended for production):
# SPRING_DATASOURCE_URL=jdbc:postgresql://your-endpoint.neon.tech/neondb?sslmode=require
# SPRING_DATASOURCE_USERNAME=neondb_owner
# SPRING_DATASOURCE_PASSWORD=your_neon_password

# OAuth2 Google Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# OAuth2 GitHub Configuration
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_minimum_256_bits

# Landing Page URL
LANDING_PAGE_URL=http://localhost:9090/login/oauth2/code/google
